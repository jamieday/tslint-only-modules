{
  "compilerOptions": {
    "lib": ["es2015"],
    "target": "es5",
    "module": "commonjs",
    "importHelpers": true,
    "noUnusedParameters": true,
    "noUnusedLocals": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "suppressImplicitAnyIndexErrors": true,
    "strict": true,
    "pretty": true,
    "removeComments": true
  }
}